name = input("Enter name: ")

marks = input("Enter marks in homework, quiz, mid Term, finals: ")
m = marks.split(' ')
m1 = []
for i in m:
    m1.append(int(i))

print(m1)
wAvg = ((0.20 * m1[0]) + (0.20 * m1[1]) + (0.30 * m1[2]) + (0.30 * m1[3]) / len(m1))
print(f"Weighted average of {name} is {wAvg: .2f}%")
